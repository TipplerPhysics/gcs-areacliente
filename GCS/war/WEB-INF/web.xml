<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>GCS</display-name>

	
	
	<!-- LISTADO DE SERVLETS -->
	
	  <servlet>
	    <servlet-name>DefaultConf</servlet-name>
	    <servlet-class>com.gcs.servlet.DefaultConf</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>DefaultConf</servlet-name>
	    <url-pattern>/defaultConf</url-pattern>
	  </servlet-mapping>
	  
	   <servlet>
	    <servlet-name>LogsServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.LogsServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>LogsServlet</servlet-name>
	    <url-pattern>/logsServlet</url-pattern>
	  </servlet-mapping>
	  
	  <servlet>
	    <servlet-name>TeamServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.TeamServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>TeamServlet</servlet-name>
	    <url-pattern>/teamServlet</url-pattern>
	  </servlet-mapping>
	  
	  <servlet>
	    <servlet-name>ConectividadServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.ConectividadServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>ConectividadServlet</servlet-name>
	    <url-pattern>/conectividadServlet</url-pattern>
	  </servlet-mapping>
	
	  <servlet>
	    <servlet-name>ConectividadExcelServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.ConectividadExcelServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>ConectividadExcelServlet</servlet-name>
	    <url-pattern>/conectividadExcelServlet</url-pattern>
	  </servlet-mapping>
	  
	  <servlet>
	    <servlet-name>DemandaServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.DemandaServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>DemandaServlet</servlet-name>
	    <url-pattern>/demandaServlet</url-pattern>
	  </servlet-mapping>
	  
	   <servlet>
	    <servlet-name>CosteServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.CosteServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>CosteServlet</servlet-name>
	    <url-pattern>/costeServlet</url-pattern>
	  </servlet-mapping>
	  
	  <servlet>
	    <servlet-name>ConfigurationServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.ConfigurationServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>ConfigurationServlet</servlet-name>
	    <url-pattern>/configuration</url-pattern>
	  </servlet-mapping>
	
	  <servlet>
	    <servlet-name>UserServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.UserServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>UserServlet</servlet-name>
	    <url-pattern>/usersServlet</url-pattern>
	  </servlet-mapping>
	  
     <servlet>
	    <servlet-name>ProyectoServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.ProjectServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>ProyectoServlet</servlet-name>
	    <url-pattern>/projectServlet</url-pattern>
	  </servlet-mapping>
	  
	  <servlet>
	    <servlet-name>ServicioServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.ServicioServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>ServicioServlet</servlet-name>
	    <url-pattern>/serviceServlet</url-pattern>
	  </servlet-mapping>
	  
	   <servlet>
	    <servlet-name>ClienteServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.ClienteServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>ClienteServlet</servlet-name>
	    <url-pattern>/clienteServlet</url-pattern>
	  </servlet-mapping>
	  
	  <servlet>
	    <servlet-name>ImplantacionServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.ImplantacionServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>ImplantacionServlet</servlet-name>
	    <url-pattern>/implantacionServlet</url-pattern>
	  </servlet-mapping>
	  
	  	  <servlet>
	    <servlet-name>InformeServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.InformeServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>InformeServlet</servlet-name>
	    <url-pattern>/informeServlet</url-pattern>
	  </servlet-mapping>
	  
	  	  	  <servlet>
	    <servlet-name>InformeExcelServlet</servlet-name>
	    <servlet-class>com.gcs.servlet.InformeExcelServlet</servlet-class>
	  </servlet>
	
	  <servlet-mapping>
	    <servlet-name>InformeExcelServlet</servlet-name>
	    <url-pattern>/informeExcelServlet</url-pattern>
	  </servlet-mapping>

	  
    <security-constraint>
        <web-resource-collection>
            <web-resource-name>user</web-resource-name>
            <url-pattern>/*</url-pattern>
        </web-resource-collection>
        <auth-constraint>
            <role-name>*</role-name>
        </auth-constraint>
    </security-constraint>
    
    <!-- FILTROS -->
    
    <servlet>
		<servlet-name>action</servlet-name>
		<servlet-class>org.apache.struts.action.ActionServlet</servlet-class>
		<init-param>
			<param-name>config</param-name>
			<param-value>/WEB-INF/struts-config.xml</param-value>
		</init-param>
		<load-on-startup>2</load-on-startup>
	</servlet>

	<filter>
	    <filter-name>userFilter</filter-name>
	    <filter-class>com.gcs.filters.UserFilter</filter-class>
	</filter>
	<filter-mapping>
	    <filter-name>userFilter</filter-name>
	    <url-pattern>/*</url-pattern>
	</filter-mapping>
	
	
	<!-- 
	<filter>
	    <filter-name>ownerFilter</filter-name>
	    <filter-class>com.gcs.filters.OwnerFilter</filter-class>
	</filter>
	<filter-mapping>
	    <filter-name>ownerFilter</filter-name>
	    <url-pattern>/admin/users.do</url-pattern>
	</filter-mapping>
	 -->
	
	
	 
	<!-- ######################################## -->

	<servlet-mapping>
		<servlet-name>action</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>jsp/index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
